{{#contentFor "sidebarActions"}}
<div class="card">
    <div class="card__title">
        Actions
    </div>
    <div class="card__content">
        <ul class="card__action_list">
            <li><a href="/articles">Back to Articles</a></li>
        </ul>
    </div>
</div>
{{/contentFor}}

<div class="section">
    <h2 class="section__title">New Article</h2>
    <div class="section__content">
        {{> gform
        actionUrl=actionUrl
        isEdit=false
        submitButtonText="Post"
        titleName="Title"
        contentName="Content"
        }}
    </div>
</div>